        [_unit,_westFaction] call setID;
    };
    if (side _unit == east) then {
        [_unit,_eastfaction] call setID;
    };
    if (side _unit == independent) then {
        [_unit,_indFaction] call setID;
    };
    if (side _unit == civilian) then {
        [_unit,_civFaction] call setID;
    };

